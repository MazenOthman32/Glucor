{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 116",
    "not and_ff <= 116",
    "not chrome <= 116",
    "not edge <= 116",
    "not firefox <= 116",
    "not ios_saf <= 16.5",
    "not opera <= 101",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}